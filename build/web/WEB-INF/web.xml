<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AdminClientUpdate</servlet-name>
        <servlet-class>AdminClientUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientPro</servlet-name>
        <servlet-class>ClientPro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientReg</servlet-name>
        <servlet-class>ClientReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getRecord</servlet-name>
        <servlet-class>MyPack.getRecord</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gen</servlet-name>
        <servlet-class>gen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RSum</servlet-name>
        <servlet-class>MyPack.RSum</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sum</servlet-name>
        <servlet-class>MyPack.Sum</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>email_service</servlet-name>
        <servlet-class>MyPack.email_service</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyMail</servlet-name>
        <servlet-class>MyMail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>send_message</servlet-name>
        <servlet-class>send_message</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Generate_PDF</servlet-name>
        <servlet-class>MyPack.Generate_PDF</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminClientUpdate</servlet-name>
        <url-pattern>/AdminClientUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientPro</servlet-name>
        <url-pattern>/ClientPro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientReg</servlet-name>
        <url-pattern>/ClientReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getRecord</servlet-name>
        <url-pattern>/getRecord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gen</servlet-name>
        <url-pattern>/gen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RSum</servlet-name>
        <url-pattern>/RSum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sum</servlet-name>
        <url-pattern>/Sum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>email_service</servlet-name>
        <url-pattern>/email_service</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyMail</servlet-name>
        <url-pattern>/MyMail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>send_message</servlet-name>
        <url-pattern>/send_message</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Generate_PDF</servlet-name>
        <url-pattern>/Generate_PDF</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
